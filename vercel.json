{
    "version": 2,
    "builds": [
        { 
            "src": "dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ],
    "scripts": {
        "build": "npm install",
        "start": "node dist/index.js",
        "dev": "npx nodemon"
    }
}